[include]
  path = ~/.gitconfig_private

[core]
  autocrlf = false
  safecrlf = true
  excludesfile = /Users/kristinahjertberg/.gitignore_global

[alias]
  co = checkout
  ap = add -p
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  last = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short -n
  type = cat-file -t
  dump = cat-file -p
  undo = reset --soft HEAD~1

[color]
  diff = auto
  status = auto
  branch = auto
